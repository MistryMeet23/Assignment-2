<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Uploaded Files</title>
</head>
<body>
  <h1>Uploaded Files</h1>
  <ul>
    <% users.forEach(user => { %>
      <% user.files.forEach(file => { %>
        <li>
          <a href="/download/<%= file %>" download><%= file %></a>
        </li>
      <% }) %>
    <% }) %>
  </ul>
</body>
</html>
